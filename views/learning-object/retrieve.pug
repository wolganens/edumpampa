extends ../layout

block content
  h3
    | Meus Objetos de Aprendizagem
  table.table
    thead
      th
        | Título
      th
        | Situação
      th
        | Ações
    tbody
      each lo in data
        tr
          td
            a(title=lo.title target="_blank" href=`/learning-object/single/${lo._id}`) #{lo.title}            
          td
            | !{lo.htmlSituation}
          td
            .btn-group
              a.btn.btn-default(href="/learning-object/remove/" + lo._id)
                | Apagar

